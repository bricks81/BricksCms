{
    "name": "bricks81/bricks-cms",
    "description": "Bricks CMS Framework - CMS Implementation",
    "license": "MIT",
    "keywords": [
        "bricks",
        "cms"
    ],
    "homepage": "https://www.bricks-cms.org",
    "autoload": {
        "psr-4": {
            "Bricks\\Cms\\": "src/"
        }
    },
    "require": {
        "php": "^7.0",
        "components/jquery":"*",
        "components/jqueryui":"*",
        "components/font-awesome":"*",
        "zendframework/zend-db":"*",
        "zendframework/zend-i18n-resources":"*",
        "zendframework/zend-session":"*",
        "zendframework/zend-navigation":"*",
        "bricks81/bricks-exception":"dev-master",
        "bricks81/bricks-file":"dev-master",
        "bricks81/bricks-config":"dev-master",
        "bricks81/bricks-loader":"dev-master",
        "bricks81/bricks-translate":"dev-master",
        "bricks81/bricks-download":"dev-master",
        "bricks81/bricks-upload":"dev-master",
        "bricks81/bricks-module":"dev-master",
        "bricks81/bricks-model":"dev-master",
        "bricks81/bricks-navigation":"dev-master",
        "bricks81/bricks-acl":"dev-master",
        "bricks81/bricks-asset":"dev-master",
        "bricks81/bricks-backup":"dev-master",
        "bricks81/bricks-update":"dev-master",
        "bricks81/bricks-user":"dev-master"
    },
    "minimum-stability": "dev",
    "prefer-stable": false    
}
